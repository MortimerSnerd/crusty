enum ObjectKind:short { okInvalid = 0, 
  Player, 
  Sword
}

enum TileName:int32 {
  None = -1,
  Wally = 200,
  Sword = 554,
  Blondie = 905
}


enum ItemFlags : uint32 (bit_flags)
{
    Living,
    BlocksLiving
}

table ObjectSpec {
   kind: ObjectKind;
   short_name: string;
   static_tile: TileName = None;
   weight: float;
   flags: ItemFlags;
   right_rotation: float = 0;
} 

table ConfigRoot {
   objtypes: [ObjectSpec];
}
    
root_type ConfigRoot;

